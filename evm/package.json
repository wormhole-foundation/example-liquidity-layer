{
  "name": "@wormhole-foundation/example-liquidity-layer-evm",
  "version": "0.0.1",
  "author": "Wormhole Contributors",
  "license": "Apache-2.0",
  "main": "./dist/cjs/index.js",
  "types": "./dist/cjs/index.d.ts",
  "module": "./dist/esm/index.js",
  "files": [
    "dist/cjs",
    "dist/esm"
  ],
  "scripts": {
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build": "npm run build:esm && npm run build:cjs",
    "generate": "typechain --target=ethers-v5 --out-dir=ts/src/types out/[!build-info]*/*.json",
    "clean": "rm -rf dist && rm -rf node_modules && rm -f ./*.tsbuildinfo"
  },
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/cjs/index.d.ts"
    },
    "./*": {
      "import": "./dist/esm/*/index.js",
      "require": "./dist/cjs/*/index.js",
      "types": "./dist/cjs/*/index.d.ts"
    }
  },
  "dependencies": {
    "@wormhole-foundation/example-liquidity-layer-definitions": "0.0.1",
    "@wormhole-foundation/sdk-base": "^0.10.9",
    "@wormhole-foundation/sdk-definitions": "^0.10.9",
    "@wormhole-foundation/sdk-evm": "^0.10.9"
  },
  "devDependencies": {
    "@typechain/ethers-v5": "^10.2.0",
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.14.5",
    "chai": "^4.3.7",
    "dotenv": "^16.3.1",
    "envfile": "^7.1.0",
    "ethers": "^5.7.2",
    "mocha": "^10.0.0",
    "prettier": "^2.8.7",
    "prettier-plugin-solidity": "^1.1.3",
    "ts-mocha": "^10.0.0",
    "typechain": "^8.1.1"
  }
}
